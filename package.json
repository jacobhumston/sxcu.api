{
    "name": "sxcu.api",
    "version": "1.3.0",
    "description": "Node.js library to interact with the sxcu.net API.",
    "main": "build/index.js",
    "types": "build/index.d.ts",
    "type": "module",
    "maintainers": [
        {
            "name": "Jacob Humston",
            "email": "lovelyjacob@aol.com"
        }
    ],
    "scripts": {
        "test": "node test/index.js",
        "test-other": "node test/other.js",
        "build": "npx del-cli build/ && npx tsc --outDir build",
        "eslint": "npx eslint build/",
        "gen-doc": "npx del-cli docs/ && npx typedoc --cname sxcu.api.lovelyjacob.com --searchInComments --hideGenerator && node tools/gen-web.cjs",
        "compile": "npm run build && npm run gen-doc && npm run format && npm run eslint",
        "format": "npx prettier ./ --write"
    },
    "files": [
        "build",
        "LICENSE"
    ],
    "repository": {
        "type": "git",
        "url": "git+https://github.com/Lovely-Experiences/sxcu.api.git"
    },
    "keywords": [
        "sxcu.net",
        "sxcu",
        "wrapper",
        "library",
        "api"
    ],
    "author": "jacobhumston",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/Lovely-Experiences/sxcu.api/issues"
    },
    "homepage": "https://github.com/Lovely-Experiences/sxcu.api#readme",
    "devDependencies": {
        "@types/node": "^20.8.2",
        "del-cli": "^5.1.0",
        "eslint": "^8.50.0",
        "eslint-config-prettier": "^9.0.0",
        "eslint-plugin-prettier": "^5.0.0",
        "markdown-it": "^13.0.2",
        "prettier": "^3.0.3",
        "typedoc": "^0.25.1",
        "typescript": "^5.2.2"
    }
}
