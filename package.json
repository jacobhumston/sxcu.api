{
    "name": "sxcu.api",
    "version": "1.3.0",
    "description": "Node.js library to interact with the sxcu.net API.",
    "main": "build/index.js",
    "types": "build/index.d.ts",
    "type": "module",
    "maintainers": [
        {
            "name": "Jacob Humston",
            "email": "lovelyjacob@aol.com"
        }
    ],
    "scripts": {
        "test": "node test/index.js",
        "build": "npx del-cli build/ && npx tsc --outDir build",
        "format": "npx prettier src/ --write",
        "format-build": "npx prettier build/ --write",
        "format-test": "npx prettier test/ --write",
        "eslint": "npx eslint build/",
        "gen-doc": "npx del-cli docs/ && npx typedoc src/index.ts",
        "compile": "npm run build && npm run format-build && npm run gen-doc && npm run eslint",
        "format-all": "npm run format && npm run format-build && npm run format-test"
    },
    "files": [
        "build",
        "types",
        "LICENSE"
    ],
    "repository": {
        "type": "git",
        "url": "git+https://github.com/Lovely-Experiences/sxcu.api.git"
    },
    "keywords": [
        "sxcu.net",
        "sxcu",
        "wrapper",
        "library",
        "api"
    ],
    "author": "jacobhumston",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/Lovely-Experiences/sxcu.api/issues"
    },
    "homepage": "https://github.com/Lovely-Experiences/sxcu.api#readme",
    "devDependencies": {
        "@types/node": "^20.8.2",
        "del-cli": "^5.1.0",
        "eslint": "^8.50.0",
        "eslint-config-prettier": "^9.0.0",
        "eslint-plugin-prettier": "^5.0.0",
        "mocha": "^10.2.0",
        "prettier": "^3.0.3",
        "typedoc": "^0.25.1",
        "typescript": "^5.2.2"
    }
}
