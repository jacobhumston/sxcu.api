{
    "name": "sxcu.api",
    "version": "1.2.5",
    "description": "Node.js library to interact with the sxcu.net API.",
    "main": "src/index.js",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/Lovely-Experiences/sxcu.api.git"
    },
    "types": "./dist/index.d.ts",
    "scripts": {
        "generate-doc": "./node_modules/.bin/jsdoc ./src/ -d ./docs/ -R ./README.md -c ./jsdoc.json -u ./tutorials/ -r --verbose",
        "test": "node ./test/index.js",
        "local-node-test": "./node/bin/node ./test/index.js",
        "check": "./node_modules/.bin/eslint src/",
        "generate-types": "./node_modules/.bin/tsc",
        "generate-doc-windows": ".\\node_modules\\.bin\\jsdoc .\\src\\ -d .\\docs\\ -R .\\README.md -c .\\jsdoc.json -u .\\tutorials\\ -r --verbose",
        "check-windows": ".\\node_modules\\.bin\\eslint src\\",
        "generate-types-windows": ".\\node_modules\\.bin\\tsc"
    },
    "keywords": [
        "sxcu.net",
        "sxcu",
        "wrapper",
        "library",
        "api"
    ],
    "directories": {
        "lib": "/src",
        "test": "/test"
    },
    "author": "Jacob Humston",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/Lovely-Experiences/sxcu.api/issues"
    },
    "homepage": "https://github.com/Lovely-Experiences/sxcu.api#readme",
    "devDependencies": {
        "clean-jsdoc-theme": "^4.2.5",
        "eslint": "^8.32.0",
        "eslint-config-prettier": "^8.6.0",
        "eslint-plugin-prettier": "^4.2.1",
        "jsdoc": "^4.0.0",
        "prettier": "^2.8.3",
        "typescript": "^4.9.4"
    }
}
